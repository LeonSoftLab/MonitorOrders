      'set @EndDate = :EndDate'
      'set @idEmp = :idEmp'
      ''
      
        'set @StartDate = convert(datetime,convert(nvarchar(10),@StartDat' +
        'e,103),103)'
      
        'set @EndDate = convert(datetime,convert(nvarchar(10),@EndDate,10' +
        '3)+'#39' 23:59:59'#39',103)'
      ''
      'create table #filtrPos'
      '('
      ' [id] bigint,'
      ' [idParent] bigint,'
      ' [deleted] bit,'
      ' [outercode] nvarchar(255)'
      ');'
      ''
      'create table #tmpDocs'
      '('
      ' id int,'
      ' idChicago bigint,'
      ' OTChID int,'
      ' OpType int'
      ');'
      ''
      'create table #tmpSumsDocsOT'
      '('
      ' id int,'
      ' Qty int,'
      ' Summ float'
      ');'
      ''
      'create table #tmpSumsDocsCH'
