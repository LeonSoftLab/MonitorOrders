        ' inner join [s-sql-d3].[atlantis].[dbo].[t_invD] invd with(noloc' +
        'k) on invd.chid = inv.chid'
      
        'where d.OpType = 0 and inv.DocDate between @StartDate-3 and @End' +
        'Date+7'
