      ''
      'WITH a ([id], [idParent], [deleted], [outercode]) AS'
      '( '
      '  SELECT rP.[id], rP.[idParent], rP.[deleted], rP.[outercode]'
      '  FROM [chicago].[dbo].[refPhysicalPersons] rPP with(nolock)'
      
