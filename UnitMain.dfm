  object ADOQuery_GetRight: TADOQuery
    Connection = ADOConnection
    Parameters = <
      item
        Name = 'DomainName'
        DataType = ftString
        Size = -1
        Value = ''
      end>
    SQL.Strings = (
      'declare @DomainName nvarchar(255)'
      ''
      'set @DomainName = :DomainName'
      ''
      'select distinct'
      ' rE.Id,'
      ' rE.FIO,'
      ' rE.DomainName,'
      ' rE.FirstName,'
      ' rP.Name as [PositionName],'
      ' rE.IdCurrentPosition,'
      
        ' cast(max(case when vrP.idRole in (2,291,299,300,23,32,28,39) th' +
        'en 1 else 0 end) as bit) as [useOrders],'
      
        ' cast(max(case when vrP.idRole in (2,291,299,300,189,301,28) the' +
        'n 1 else 0 end) as bit) as [useGPS]'
      'FROM [SED].[dbo].[refEmployee] rE with(nolock)'
      
        ' inner join [SED].[dbo].[refPositions] rP with(nolock) on rP.id ' +
        '= rE.IdCurrentPosition'
      
        ' inner join [SED].[dbo].[v_refPositions] vrP on rE.Id = case whe' +
        'n vrP.idAssistantEmployee=0 then vrP.idEmployee else vrP.idAssis' +
        'tantEmployee end'
      'where rE.[DomainName]=@DomainName'
      'group by  rE.Id,'
      ' rE.FIO,'
      ' rE.DomainName,'
      ' rE.FirstName,'
      ' rP.Name,'
      ' rE.IdCurrentPosition')
    Left = 455
    Top = 75
