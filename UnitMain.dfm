        '   inner join [workdb4_7].[dbo].[DocJournal] dj with(nolock) on ' +
        'djCh.id = dj.idChicago'
      '  where dj.[DateProcessing] between @StartDate and @EndDate'
      '  order by dj.[DateProcessing]'
      ''
      'insert into #tmpSumsDocsOT'
